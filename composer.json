{
    "name": "setono/doctrine-orm-batcher",
    "description": "A library for processing large collections in Doctrine",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": ">=7.4",
        "doctrine/collections": "^1.6",
        "doctrine/orm": "^2.6",
        "doctrine/persistence": "^1.3 || ^2.1",
        "symfony/property-access": "^5.4 || ^6.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "doctrine/annotations": "^2.0",
        "doctrine/data-fixtures": "^1.5",
        "doctrine/orm": "^2.8",
        "phpunit/phpunit": "^9.5.10",
        "psalm/plugin-phpunit": "^0.16.1",
        "setono/code-quality-pack": "^2.1.3",
        "symfony/cache": "^5.4 || ^6.0",
        "weirdan/doctrine-psalm-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Setono\\DoctrineORMBatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Setono\\DoctrineORMBatcher\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
